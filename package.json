    "dev": "ts-node src/app.ts"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.3",
    "body-parser": "^1.19.0",
    "cross-var": "^1.1.0",
    "express": "^4.17.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3",
    "winston": "^3.2.1"
  }
}
